<?xml version="1.0"?>
<odoo>
    <record id="property_offer_model_menu_action" model="ir.actions.act_window">
        <field name="name">Property Types</field>
        <field name="res_model">property_offer</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="property_offer_tree_view" model="ir.ui.view">
        <field name="name">property_offer.tree</field>
        <field name="model">property_offer</field>
        <field name="arch" type="xml">
            <tree string="Offers">
                <field name="selling_price" />
                <field name="buyer_id" />
                <field name="status" />
                <field name="validity"/>
                <field name="date_deadline"/>
            </tree>
        </field>
    </record>
</odoo>